#include <stdio.h>

int main(void)
{
	double charge, chargeE;
	double electrons;

	printf("Enter the charge:");
	scanf("%lf", &charge);

	printf("Enter the charge of an electron:");
	scanf("%le", &chargeE);


	electrons = (charge / chargeE) * -1;

	printf("Total number of electrons:");

	printf("Press enter key to exit application \n");

	while(getchar() != '\n')
	{

	}


	getchar();
}
