#include<stdio.h>


long long int g_data = 0xFFFEABCD11112345;

int main(void )
{
	char *pAddress1;

	pAddress1 = (char*) &g_data;
	printf("Value at Address %p is: %d\n", pAddress1, *pAddress1);

	int *pAddress2;

	pAddress2 = (int*) &g_data;
	printf("Value at Address %p is: %d\n", pAddress2, *pAddress2);


	while(getchar() != '\n')
	{

	}


	getchar();

	return 0;
}
